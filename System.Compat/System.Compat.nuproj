<Project Sdk="Microsoft.Build.NoTargets">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
    </PropertyGroup>
    <ItemGroup>
        <Content Include="**/*.cs" PackagePath="contentFiles/$(TargetFramework)/%(RecursiveDir)%(Filename)%(Extension)" Pack="true" />
    </ItemGroup>
</Project>
